    "Strategic_Ploys": [
        {
            "Name": "Bolter Discipline",
            "Cost": 1,
            "Content": [
                "Until the end of the Turning Point, each time a friendly <span class='killteam-text'>Firstborn Astartes</span> operative is activated, if it does not perform a <b>Fight</b> action during that activation, it can perform two <b>Shoot</b> actions during that activation if a bolt weapon is selected for each of those shooting attacks."
            ]
        },
        {
            "Name": "Shock Assault",
            "Cost": 1,
            "Content": [
                "Until the end of the Turning Point, each time a friendly <span class='killteam-text'>Firstborn Astartes</span> operative is activated, if it does not perform a <b>Shoot</b> action during that activation, it can perform two <b>Fight</b> actions during that activation."
            ]
        },
        {
            "Name": "Combat Doctrines",
            "Cost": 1,
            "Content": [
                "Select one of the following Doctrines. Selected doctrine will be active until the end of the Turning Point. While Doctrine is active, friendly <span class='killteam-text'>Firstborn Astartes</span> operative's weapons gain balanced Special Rule while:",
                [
                    {
                        "Text": "<b>Devastator Doctrine:</b> That operative is making a shooting attack against an enemy operative more than <span class='f6'></span> from it."
                    },
                    {
                        "Text": "<b>Tactical Doctrine:</b> That operative is making a shooting attack against an enemy operative less than <span class='f6'></span> from it."
                    },
                    {
                        "Text": "<b>Assault Doctrine:</b> That operative is fighting in combat."
                    }
                ]
            ]
        }
    ],
    "Tactical_Ploys": [
        {
            "Name": "Only in Death Does Duty End",
            "Cost": 2,
            "Content": [
                "Use this Tactical Ploy when a ready friendly <span class='killteam-text'>Firstborn Astartes</span> operative is incapacitated. Before removing that operative from the killzone, place a Last Stand token under its base. Next rules apply to Last Stand token:",
                [
                    {
                        "Text": "Last Stand token may capture points and tokens as if it had an APL of 3."
                    },
                    {
                        "Text": "Enemy operatives within <span class='f1'></span> of Last Stand token are considered to be in Engagement Range."
                    }
                ],
                "Remove Last Stand token at the start of the next Turning Point or the end of the battle (whichever comes first)."
            ]
        },
        {
            "Name": "No Mercy, No Respite",
            "Cost": 1,
            "Content": [
                "Use this Tactical Ploy when is your turn to activate an operative and you do not have any ready operatives left to activate. Select one friendly <span class='killteam-text'>Firstborn Astartes</span> operative, that operative may perform two <b>Overwatch</b> actions during this Turning Point."
            ]
        }
    ],
